{
    "name": "api",
    "version": "1.0.0",
    "description": "Simple Express API",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "nodemon --exec ts-node --transpile-only src/server.ts",
        "type-check": "tsc --noEmit",
        "migrate": "ts-node-dev --transpile-only src/migrations/run-migrations.ts",
        "migrate:prod": "node dist/migrations/run-migrations.js"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.490.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "ethers": "^6.16.0",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "nodemon": "^3.1.11",
        "pg": "^8.11.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.11.0",
        "@types/pg": "^8.10.9",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    }
}